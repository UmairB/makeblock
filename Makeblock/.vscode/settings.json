// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
        "/**/*.js": {
            "when": "$(basename).ts"
        },
        "/**/*.js.map": true,
        "/**/*.css": {
            "when": "$(basename).scss"
        },
        "/**/*.css.map": true,
        "node_modules": true
    }
}